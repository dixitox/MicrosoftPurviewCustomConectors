# Environment variables template for Database Connector Example
# Copy this to .env and fill in your values

# Purview Configuration
PURVIEW_ACCOUNT_NAME=your-purview-account
AZURE_TENANT_ID=00000000-0000-0000-0000-000000000000
AZURE_CLIENT_ID=00000000-0000-0000-0000-000000000000
AZURE_CLIENT_SECRET=your-client-secret

# Database Configuration
DB_TYPE=sql_server  # Options: sql_server, postgresql, mysql
DB_SERVER=your-server.database.windows.net
DB_NAME=your-database
DB_USER=your-username
DB_PASSWORD=your-password

# Optional: Purview Collection
COLLECTION_NAME=DatabaseAssets
